ate, { getFirebase }) => {\n    const firebase = getFirebase();\n    firebase\n      .logout()\n      .then(() => {\n        dispatch({ type: LOGOUT_SUCCESS });\n      })\n      .catch((err) => {\n        dispatch({ type: LOGOUT_ERROR });\n      });\n  };\n};\n\n// export const signUpWithGoogle = () => {\n//   return (dispatch, getState, { getFirebase }) => {\n//     const firebase = getFirebase();\n//     firebase\n//       .login({\n//         provider: 'google',\n//         type: 'redirect',\n//       })\n//       .then(() => {\n//         dispatch({ type: LOGIN_SUCCESS });\n//       })\n//       .catch((err) => {\n//         dispatch({ type: LOGIN_ERROR });\n//       });\n//   };\n// };\n","/Users/tomona_s/Documents/06_web work/recipe-app/src/components/pages/Create.js",[],"/Users/tomona_s/Documents/06_web work/recipe-app/src/components/organisms/CartDetail.js",[],["151","152","153","154","155"],"/Users/tomona_s/Documents/06_web work/recipe-app/src/components/atoms/LabelWithIcon.js",[],"/Users/tomona_s/Documents/06_web work/recipe-app/src/components/pages/Detail.js",[],"/Users/tomona_s/Documents/06_web work/recipe-app/src/components/atoms/MenuForRecipe.js",[],"/Users/tomona_s/Documents/06_web work/recipe-app/src/components/atoms/RecipeInstruction.js",[],"/Users/tomona_s/Documents/06_web work/recipe-app/src/components/organisms/Header.js",[],"/Users/tomona_s/Documents/06_web work/recipe-app/src/components/App.js",[],"/Users/tomona_s/Documents/06_web work/recipe-app/src/components/atoms/IngredientLabel.js",[],"/Users/tomona_s/Documents/06_web work/recipe-app/src/components/pages/Page404.js",[],"/Users/tomona_s/Documents/06_web work/recipe-app/src/components/organisms/Footer.js",[],"/Users/tomona_s/Documents/06_web work/recipe-app/src/utils/formConverter.js",[],"/Users/tomona_s/Documents/06_web work/recipe-app/src/modules/recipes.js",["156","157","158","159","160"],"/Users/tomona_s/Documents/06_web work/recipe-app/src/components/molecules/LogIn.js",[],"/Users/tomon